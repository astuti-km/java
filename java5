import java.util.*;
class Main{
    public static void main(String args[]){
        Scanner in = new Scanner(System.in);
        int n=in.nextInt();
        int original =n;
        int rev=0;
        while(n>0){
            int p=n%10;
            rev=rev*10+p;
            n/=10;
        }
        System.out.println("The reverse is: "+rev);
    }}
