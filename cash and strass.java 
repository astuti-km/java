import java.util.*;

public class CashierAlgorithm {

    public static void main(String[] args) {
        int[] coins = {2000, 500, 200, 100, 50, 20, 10, 5, 2, 1};
        int amount = 2893;

        System.out.println("Amount: " + amount);
        System.out.println("Coins used:");

        for (int i = 0; i < coins.length; i++) {
            if (amount >= coins[i]) {
                int count = amount / coins[i];
                amount = amount % coins[i];

                System.out.println(coins[i] + " x " + count);
            }
        }
    }
}


public class Strassen2x2 {

    public static void main(String[] args) {

        int[][] A = { {1, 2}, {3, 4} };
        int[][] B = { {5, 6}, {7, 8} };

        int a = A[0][0];
        int b = A[0][1];
        int c = A[1][0];
        int d = A[1][1];

        int e = B[0][0];
        int f = B[0][1];
        int g = B[1][0];
        int h = B[1][1];

        int M1 = (a + d) * (e + h);
        int M2 = (c + d) * e;
        int M3 = a * (f - h);
        int M4 = d * (g - e);
        int M5 = (a + b) * h;
        int M6 = (c - a) * (e + f);
        int M7 = (b - d) * (g + h);

        int C11 = M1 + M4 - M5 + M7;
        int C12 = M3 + M5;
        int C21 = M2 + M4;
        int C22 = M1 - M2 + M3 + M6;

        System.out.println("Result Matrix:");
        System.out.println(C11 + " " + C12);
        System.out.println(C21 + " " + C22);
    }
}